{
  "name": "meditime-ertesito",
  "version": "5.0.0",
  "author": "mgabor",
  "scripts": {
    "dev": "node --icu-data-dir=node_modules/full-icu -r dotenv/config -r ts-node/register src/index.ts",
    "build": "tsc",
    "update": "git pull && yarn && yarn build",
    "start": "node --icu-data-dir=node_modules/full-icu -r dotenv/config dist/index.js",
    "start:dry": "yarn cross-env NODE_ENV=development yarn start",
    "lint": "yarn run eslint --max-warnings=0 . && yarn run prettier --write ."
  },
  "dependencies": {
    "@t-botz/pino-rollbar-transport": "^1.0.0",
    "async-retry": "^1.3.3",
    "dotenv": "^10.0.0",
    "email-templates": "^11.1.1",
    "full-icu": "^1.5.0",
    "googleapis": "^74.2.0",
    "lodash": "^4.17.21",
    "object-hash": "^2.2.0",
    "pino": "^8.17.1",
    "pino-pretty": "^10.3.0",
    "puppeteer": "^21.6.1",
    "rollbar": "^2.26.2",
    "ts-node": "^10.9.2",
    "weeknumber": "^1.2.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/async-retry": "^1.4.8",
    "@types/email-templates": "^10.0.4",
    "@types/lodash": "^4.14.202",
    "@types/node": "^18.19.3",
    "@types/object-hash": "^2.2.1",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "private": true,
  "packageManager": "yarn@4.0.2"
}
